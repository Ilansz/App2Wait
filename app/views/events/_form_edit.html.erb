<%= form_for(@event) do |f| %>
  <%= f.fields_for :events_levels, @event.events_levels.sort_by(&:time) do |t|  %>
  <%= render 'event_levels_fields', t: t %>
  <% end %>
  <div class="d-flex justify-content-center">
    <%= f.button :submit, class: "btn btn-outline-warning w-100 font-weight-bolder" %>
  </div>
<% end %>
